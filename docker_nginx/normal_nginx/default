# normal CGI server

server {
    listen 127.0.0.3:8080;
    listen 127.0.0.1:8080;
#    listen [::]:8080 default_server;

    root /var/www/html;
    autoindex on;

    index index.html index.htm index.nginx-debian.html;
    server_name A;
    
    location / {
        try_files $uri $uri/ =404;
    }

    include fcgiwrap.conf;
}

server {
    listen 127.0.0.3:8080;
    listen 127.0.0.2:8080;
#    listen [::]:8080 default_server;

    root /var/www/html;
    autoindex on;

    index index_dummy.html index.htm index.nginx-debian.html;
    server_name B;
    
    location / {
        try_files $uri $uri/ =404;
    }

    include fcgiwrap.conf;
}

